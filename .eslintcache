-end/src/components/dashboard/pages/ToReview.jsx",["110"],"import React, {useEffect, useState} from 'react';\nimport SinglePublicationCard from \"../../utilities/SinglePublicationCard\";\nimport PublicationService from \"../../../services/dashboard/Publication.Service\";\nimport PublicationDialog from \"./PublicationDialog\";\nimport MessageDialog from \"../../utilities/MessageDialog\";\n\nfunction ToReview(props) {\n\n    const [isPublicationDialogOpen, setIsPublicationDialogOpen] = useState(false);\n    const [clickedPublication, setClickedPublication] = useState({\n            uuid:\"\",\n            title:\"\",\n            author:\"\",\n            publicationHouse:\"\",\n            abstract:\"\",\n            detail:\"\",\n            approved:false,\n            fileId:\"\",\n            publicationType:\"\"\n        }\n    );\n\n    const [datas, setDatas] = useState([{}]);\n    const [reload, setReload] = useState(false);\n\n    const [isMessageDialogOpen, setIsMessageDialogOpen] = useState(false);\n\n    useEffect(()=>{\n        console.log(\"to review\"+ props.role)\n        if(props.role !== null){\n            PublicationService.getPublicationsToReview(props.role).then((response)=>{\n                setDatas(response)\n                console.log(response)\n            }).catch(e=>console.log(e))\n        }\n\n    },[reload])\n\n    useEffect(()=>{\n        if(datas ===null || datas.length===0){\n            setIsMessageDialogOpen(true)\n        }\n    },[datas])\n\n    function publicationClickHandler(uuid){\n        setIsPublicationDialogOpen(true)\n\n        let data = datas.filter((data)=>data.uuid === uuid)[0]\n        console.log(data)\n        setClickedPublication({\n            title: data.title,\n            publicationHouse: data.publicationHouse,\n            uuid:uuid,\n            author:data.authorId,\n            abstract: data.abst,\n            detail:data.detail,\n            approved: data.approved,\n            fileId: data.fileId,\n            publicationType: data.publicationType\n        })\n        console.log(clickedPublication.title)\n        // setClickedPublication(fake)\n        console.log(uuid)\n    }\n    return (\n        <div style={{\n            display:'grid',\n            marginTop:'64px',\n            marginLeft:'32px',\n            alignItems:'center',\n            gridTemplateColumns: '1fr 1fr 1fr 1fr'\n\n        }}>\n            {\n                datas.length>0 ? datas.map((data)=>\n                    <div key={data.uuid} onClick={()=>publicationClickHandler(data.uuid)}>\n                        <SinglePublicationCard data={data} />\n                    </div>\n                ) : <MessageDialog topic={\"ALERT\"} message={\"No publication to review\"} isMessageDialogOpen={isMessageDialogOpen} setIsMessageDialogOpen={setIsMessageDialogOpen}/>\n            }\n            <PublicationDialog isPublicationDialogOpen={isPublicationDialogOpen}\n                               setIsPublicationDialogOpen={setIsPublicationDialogOpen}\n                               clickedPublication={clickedPublication} setReload={setReload}\n                                isReview={true}\n                               role={props.role}\n            />\n        </div>\n    );\n}\n\nexport default ToReview;\n",["111","112"],"/home/hubbleloo/Documents/Workspace/idea-hub-foront-end/src/components/utilities/MessageDialog.jsx",[],"/home/hubbleloo/Documents/Workspace/idea-hub-foront-end/src/components/dashboard/pages/MyPublications.jsx",[],"/home/hubbleloo/Documents/Workspace/idea-hub-foront-end/src/services/auth/Auth-Header.js",[],"/home/hubbleloo/Documents/Workspace/idea-hub-foront-end/src/services/dashboard/Publication.Service.js",["113"],"import axios from '../axiosConfig';\nlet PUB = \"/publications/\"\n\nconst getApprovedPublications=async ()=>{\n    return await axios.get(PUB).then((response)=>{\n        console.log(response.data)\n        return response.data\n    }).catch((error)=>{\n        console.log(error)\n    })\n};\n\nconst getPublications=async (id)=>{\n    return axios.get(PUB+id).then((response)=>{\n        console.log(response.data)\n        return response.data\n    }).catch(e=>console.log(e))\n}\n\nconst getMyPublications=async (role)=>{\n\n    console.log(role)\n    if(role ===\"AUTHOR\"){\n        return axios.get(PUB+\"author\").then((response)=>{\n            console.log(response.data)\n            return response.data\n        }).catch(e=>console.log(e))\n    }\n    else{\n        return axios.get(PUB+\"publicationHouse\").then((response)=>{\n            return response.data\n        }).catch(e=>console.log(e))\n    }\n\n}\n\nconst getPublicationsToReview=async (role)=>{\n    if(role===\"AUTHOR\"){\n        return await axios.get(PUB+\"review/author\").then((response)=>{\n            console.log(response.data)\n            return response.data;\n        }).catch(e=>console.log(e))\n    }\n    else{\n        return await axios.get(PUB+\"review/publicationHouse\").then((response)=>{\n            console.log(response.data)\n            return response.data;\n        }).catch(e=>console.log(e))\n    }\n\n\n}\n\n\nconst updateReview=async (id,isApproved)=>{\n    return axios.put(PUB+\"reviewScore/\"+ id+\"/\"+isApproved).then((response)=>{\n        console.log(response.data)\n        return response.data;\n\n    }).catch(e=>console.log(e))\n\n}\n\nconst assignReviewers=async (publicationId, reviewers)=>{\n    return await axios.put(PUB+\"reviewer/\"+ publicationId,reviewers).then((response)=>{\n        console.log(\"assignReviewers\"+ response.data)\n    })\n}\n\nconst uploadPublications=async(data)=>{\n    return await axios.post(PUB,data,{\n        header:{\n            \"Content-Type\": \"multipart/form-data\"\n        }\n    }).then((response)=>{\n        console.log(\"upload\"+response)\n        return response.data\n    }).catch(e=>console.log(e))\n}\n\nexport default {\n    getApprovedPublications,\n    getPublications,\n    getMyPublications,\n    getPublicationsToReview,\n    updateReview,\n    assignReviewers,\n    uploadPublications\n}\n","/home/hubbleloo/Documents/Workspace/idea-hub-foront-end/src/components/landingPage/SignUp.jsx",[],"/home/hubbleloo/Documents/Workspace/idea-hub-foront-end/src/components/dashboard/navigationBar/Navigation.jsx",["114"],{"ruleId":"115","replacedBy":"116"},{"ruleId":"117","replacedBy":"118"},{"ruleId":"119","severity":1,"message":"120","line":28,"column":7,"nodeType":"121","endLine":28,"endColumn":32,"suggestions":"122"},{"ruleId":"119","severity":1,"message":"123","line":39,"column":7,"nodeType":"121","endLine":39,"endColumn":32,"suggestions":"124"},{"ruleId":"125","severity":1,"message":"126","line":1,"column":16,"nodeType":"127","messageId":"128","endLine":1,"endColumn":26},{"ruleId":"125","severity":1,"message":"129","line":3,"column":31,"nodeType":"127","messageId":"128","endLine":3,"endColumn":37},{"ruleId":"115","replacedBy":"130"},{"ruleId":"117","replacedBy":"131"},{"ruleId":"132","severity":1,"message":"133","line":30,"column":29,"nodeType":"134","endLine":34,"endColumn":21},{"ruleId":"135","severity":1,"message":"136","line":64,"column":1,"nodeType":"137","endLine":70,"endColumn":3},{"ruleId":"138","severity":1,"message":"139","line":7,"column":9,"nodeType":"140","endLine":10,"endColumn":46},{"ruleId":"115","replacedBy":"141"},{"ruleId":"117","replacedBy":"142"},{"ruleId":"115","replacedBy":"143"},{"ruleId":"117","replacedBy":"144"},{"ruleId":"119","severity":1,"message":"145","line":37,"column":7,"nodeType":"121","endLine":37,"endColumn":15,"suggestions":"146"},{"ruleId":"115","replacedBy":"147"},{"ruleId":"117","replacedBy":"148"},{"ruleId":"135","severity":1,"message":"136","line":81,"column":1,"nodeType":"137","endLine":89,"endColumn":2},{"ruleId":"138","severity":1,"message":"139","line":91,"column":33,"nodeType":"140","endLine":99,"endColumn":35},"no-native-reassign",["149"],"no-negated-in-lhs",["150"],"react-hooks/exhaustive-deps","React 